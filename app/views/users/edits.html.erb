<h1>Members#edit</h1>


<table>
    <%= form_for(@user, url:{controller: 'users', action: "update"}) do |form| %>
    　  <tr><th>名前</th>
            <td><%= form.text_field :name %></td></tr>
        <tr><th>自己紹介文</th>
            <td><%= form.text_field :self %></td></tr>
        <tr><th>写真</th>
            <td><%= form.file_field :pic %></td></tr>
        <tr><th></th>
            <td><%= form.submit "送信" %></td></tr>
        </tr>   
    <% end %>
</table>